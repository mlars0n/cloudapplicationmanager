<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.3.xsd">

    <!-- Add two new columns to construct URL test paths  -->
    <changeSet  id="0030"  author="mlarson">
        <addColumn tableName="SubDomain" >
            <column name="checkUrlPath" type="varchar(500)"/>
            <column name="scheme" type="varchar(10)"/>
            <column name="port" type="int"/>
        </addColumn>

    </changeSet>

</databaseChangeLog>